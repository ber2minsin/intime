{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "intime",
  "version": "0.1.0",
  "identifier": "com.intime.dev",
  "build": {
    "beforeBuildCommand": "npm --prefix ../www run build",
    "beforeDevCommand": "npm --prefix ../www run dev",
    "frontendDist": "../../www/dist",
    "devUrl": "http://localhost:5173"
  },
  "app": {
    "windows": [
      {
        "title": "intime",
        "width": 1080,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
  "icons/icon.ico"
    ]
  }
}